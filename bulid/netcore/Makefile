  OBJDIR = obj/linux/Release
  DEFINES += -DNDEBUG -DASIO_STANDALONE -DASIO_HAS_STD_ARRAY -DASIO_HAS_STD_TYPE_TRAITS -DASIO_HAS_STD_SHARED_PTR -DASIO_HAS_CSTDINT -DASIO_DISABLE_SERIAL_PORT -DASIO_HAS_STD_CHRONO
  INCLUDES += -I../../asio-1.10.6/include -I../../netcore
